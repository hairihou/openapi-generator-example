{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.5.0",
    "generators": {
      "v1.0.0": {
        "inputSpec": "#{cwd}/openapi/petstore.yaml",
        "generatorName": "typescript-axios",
        "output": "#{cwd}/packages/#{name}-models",
        "additionalProperties": {
          "enumPropertyNaming": "PascalCase",
          "npmName": "#{name}-models",
          "supportsES6": true,
          "withInterfaces": true
        }
      }
    }
  }
}
