{
  "name": "openapi-generator-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "typecheck": "tsc",
    "generate": "openapi-generator-cli generate",
    "format": "pnpm lint --fix && prettier --write **/*.{ts,tsx,cjs,mjs,json,md,yml,yaml}",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.37.1",
    "axios": "^1.6.8",
    "petstore-models": "link:packages/petstore-models",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@tanstack/eslint-plugin-query": "^5.35.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "msw": "^2.3.0",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "lint-staged": {
    "**/*.{ts,tsx,cjs,mjs,json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
